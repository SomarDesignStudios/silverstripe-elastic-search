{
    "name": "somardesignstudios/silverstripe-elastic-search",
    "description": "A silverstripe module providing search with elasticsearch",
    "type": "silverstripe-vendormodule",
    "require": {
        "php": ">=7.1.0",
        "elasticsearch/elasticsearch": "^7.6",
        "ramsey/uuid": "^3.9",
        "symbiote/silverstripe-queuedjobs": "^4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Somar\\Search\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist/css",
            "client/dist/js"
        ]
    }
}
